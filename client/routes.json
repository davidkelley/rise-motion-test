[
  {
    "path": "/",
    "page": "./pages/home",
    "chunk": "main",
    "data": {
      "feed": "GET $API_ENDPOINT/search"
    }
  },
  {
    "path": "/video/:id",
    "page": "./pages/video",
    "chunk": "main",
    "data": {
      "video": "GET $API_ENDPOINT/video?id=$id",
      "related": "GET $API_ENDPOINT/related?id=$id"
    }
  },
  {
    "path": "/search/:term",
    "page": "./pages/search",
    "chunk": "main",
    "data": {
      "results": "GET $API_ENDPOINT/search?search=$term"
    }
  },
  {
    "path": "/error",
    "page": "./pages/error",
    "chunk": "main"
  }
]
